<keymap version="1" name="Windows Proper Redo" parent="$default">
  <action id="$Redo">
    <keyboard-shortcut first-keystroke="shift ctrl z" />
    <keyboard-shortcut first-keystroke="shift alt back_space" />
    <keyboard-shortcut first-keystroke="ctrl y" />
  </action>
  <action id="EasyClick.Run.Fragment">
    <keyboard-shortcut first-keystroke="shift ctrl t" />
    <keyboard-shortcut first-keystroke="shift ctrl alt r" />
  </action>
  <action id="EditorDeleteLine" />
  <action id="ExpandTreeNode" />
  <action id="Graph.ZoomIn">
    <keyboard-shortcut first-keystroke="equals" />
  </action>
  <action id="cn.wind.action.NextLineAction">
    <keyboard-shortcut first-keystroke="ctrl 3" />
    <keyboard-shortcut first-keystroke="add" />
  </action>
</keymap>